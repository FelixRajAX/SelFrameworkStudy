<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="MySuite" parallel = "tests" thread-count="1">
    <test name="EdgeSuite">
        <parameter name="Browser" value="edge"/>
        <parameter name="url" value="https://www.amazon.com"/>
        <classes>
            <class name="Ngtest.AlphaTest"/>
        </classes>
    </test>
    <test name="ChromeSuite">
        <parameter name="Browser" value="chrome"/>
        <parameter name="url" value="https://www.flipkart.com/"/>
        <classes>
            <class name="Ngtest.AlphaTest"/>
        </classes>
    </test>
    <test name="FirefoxSuite">
        <parameter name="Browser" value="firefox"/>
        <parameter name="url" value="https://www.nykaa.com/"/>
        <classes>
            <class name="Ngtest.AlphaTest"/>
        </classes>
    </test>
</suite>